{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "env": {
    "es6": true
  },
  "globals": {
    "process": false,
    "console": false
  },
  "rules": {
    "no-alert": 2,
    "no-console": 1,
    "quotes": 0,
    "curly": 0,
    "strict": 1,
    "new-cap": 0,
    "camelcase": 0,
    "comma-dangle": 0,
    "comma-spacing": 1,
    "space-infix-ops": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": 1,
    "no-extra-semi": 0,
    "key-spacing": 1,
    "semi-spacing": 1,
    "eol-last": 1,
    "no-undef": 2,
    "valid-jsdoc": 0,
    "@typescript-eslint/no-unused-vars": [1, {
      "args": "none",
      "vars": "local",
      "varsIgnorePattern": "^_.*"
    }],
    "@typescript-eslint/no-explicit-any": 0
  }
}
